{
    "manifest_version": 2,
    "name": "Toggle Resist Fingerprinting",
    "version": "1.0",
    "description": "Adds a button to the navigation bar to toggle the \"resist fingerprinting\" setting.",
    "developer": {
        "name": "Aaron Papp",
        "url": "https://github.com/Aaron-P/ToggleResistFingerprinting"
    },
    "icons": {
        "24": "icon.svg",
        "32": "icon.svg",
        "48": "icon.svg",
        "64": "icon.svg",
        "96": "icon.svg"
    },
    "applications": {
        "gecko": {
            "strict_min_version": "57.0"
        }
    },
    "permissions": [
        "privacy"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.svg",
        "default_title": "Resist Fingerprinting",
        "theme_icons": [
            {
                "light": "icon-light.svg",
                "dark": "icon.svg",
                "size": 32
            }
        ]
    }
}